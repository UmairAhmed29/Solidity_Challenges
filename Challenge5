//Challenge#5;Create a smart contract to store ethers in it.Here is what you have to do, make it so that the user canâ€™t withdraw them before a specific time.


//SPDX-License-Identifier: UNLICENSED



pragma solidity ^0.8.16;

contract EtherTransfer {

    address public contractOwner;

    constructor() {

        contractOwner == msg.sender;

    }

    //function to send ether
    function sendEther() public payable {
        
    }
    //function to get balance 
    function getBalance() public view returns(uint) {
        return address(this).balance;
        
    }
    //function to withdraw ether
    function withdraw(address payable _to) public {

        require(block.timestamp > 1664580188 + 1 hours);
        require(contractOwner == _to);
        _to.transfer(address(this).balance);
        
    }
}
